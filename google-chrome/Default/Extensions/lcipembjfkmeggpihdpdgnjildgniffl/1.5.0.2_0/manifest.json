{
   "background": {
      "persistent": true,
      "scripts": [ "event.js" ]
   },
   "browser_action": {
      "default_icon": "icon128.png",
      "default_title": "Reproductor HLS & DASH"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Reproductor M3U8/MPD nativo, reproducir Url HLS y DASH en tu navegador",
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "23",
   "name": "Reproductor M3U8 - HLS + DASH Player",
   "permissions": [ "*://*/*.m3u8*", "*://*/*.mpd*", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.0.2",
   "web_accessible_resources": [ "player.html", "icon128.png" ]
}
